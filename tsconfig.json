{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"jsx": "react-jsx",
		"baseUrl": "src",
		"paths": {
			"@/assets/*": ["assets/*"],
			"@config": ["config.ts"],
			"@/components/*": ["components/*"],
			"@/content/*": ["content/*"],
			"@/layouts/*": ["layouts/*"],
			"@/pages/*": ["pages/*"],
			"@/styles/*": ["styles/*"],
			"@/utils/*": ["utils/*"],
			"@/data/*": ["data/*"]
		},
		"jsxImportSource": "react"
	}
}
